
@{
    ViewData["Title"] = "File";
    Layout = "~/Views/Shared/LayoutAdmin.cshtml";
}
@section Css{
    <link rel="stylesheet" type="text/css" href="~/css/admin/adfile.css">
    <link rel="stylesheet" type="text/css" href="~/css/admin/adprocess.css">
}
<div class="k main-form">
    <div class="k body-form">
        <span class=" k t name-file">Thông tin hồ sơ Đảng viên</span>
        <span class="t bnt-to-abroad" onclick="toAbroad()">Khai báo đi nước ngoài</span>
        <div class="k f-bd-file">
            <div class="k row-item">
                <div class="k tab-avt">
                    <div class=" k img-avt-file" id="img-avt">
                        <span class="brower-avt" onclick="getImage()">Brower</span>
                        <input id="multi-file" class="hidden" style="display:none" type="file" accept="image/*" multiple="">
                    </div>
                    <div class="k f-t-avt">
                        <span class="k t t-avt-left">Mã Đảng viên:</span>
                        <input type="text" class="k t ip-avt-t" disabled="disabled" id="madv" />
                    </div>
                    <div class="k f-t-avt">
                        <span class="k t t-avt-left">Họ tên Đảng viên:</span>
                        <input type="text" class="k t ip-avt-t" id="namedv" disabled="disabled" />
                    </div>
                </div>
                <div class="k bd-tab-body">
                    <div class="k tab-body">
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Họ tên khai sinh:<i class="warning">*</i> </span>
                            <input type="text" class="k t ip-avt-t" id="namekhaisinh" />
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Họ tên đang dùng:<i class="warning">*</i></span>
                            <input type="text" class="k t ip-avt-t" id="namedangdung" />
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Ngày tháng năm sinh:<i class="warning">*</i></span>
                            <div class="form-group">
                                <div class='input-group date' id='datepicker-birthday'>
                                    <input type='text' class="form-control" id="birthday" />
                                    <span class="input-group-addon">
                                        <span class="glyphicon glyphicon-calendar"></span>
                                    </span>
                                </div>
                            </div>
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Giới tính:</span>
                            <select class="k t ip-avt-t" id="sl-giotinh">
                                <option value="0">Nam</option>
                                <option value="1">Nữ</option>
                            </select>
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Dân tộc:<i class="warning">*</i></span>
                            <select class="k t ip-avt-t" id="sl-nation"></select>
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Tôn giáo:<i class="warning">*</i></span>
                            @*<input type="text" class="k t ip-avt-t" id="tongiao" />*@
                            <select class="k t ip-avt-t" id="sl-tongiao">
                                <option value="0" selected>Không</option>
                                <option value="1">Phật giáo</option>
                                <option value="2">Công giáo</option>
                                <option value="3">Cao đài</option>
                                <option value="4">Hòa hảo</option>
                                <option value="5">Tin lành</option>
                                <option value="6">Hồi giáo</option>
                                <option value="7">Ấn Độ giáo</option>
                                <option value="8">Tôn giáo khác</option>
                            </select>
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">CMND:<i class="warning">*</i></span>
                            <input type="text" class="k t ip-avt-t" maxlength="12" id="cmnd" />
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Ngày cấp:</span>
                            <div class="form-group">
                                <div class='input-group date' id='datepicker-daycmnd'>
                                    <input type='text' class="form-control" id="daycmnd" />
                                    <span class="input-group-addon">
                                        <span class="glyphicon glyphicon-calendar"></span>
                                    </span>
                                </div>
                            </div>
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Nơi cấp:<i class="warning">*</i></span>
                            <input type="text" class="k t ip-avt-t" id="noicapcmnd" />
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Quê quán:<i class="warning">*</i></span>
                            <input type="text" class="k t ip-avt-t" id="quequan" />
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Hộ khẩu thường trú:<i class="warning">*</i></span>
                            <input type="text" class="k t ip-avt-t" id="hokhauthuongtru" />
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Hôn nhân:</span>
                            <select class="k t ip-avt-t" id="sl-honnhan">
                                <option value="0">Đã có gia đình</option>
                                <option value="1">Chưa có gia đình</option>
                            </select>
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Sức khỏe:<i class="warning">*</i></span>
                            <input type="text" class="k t ip-avt-t" id="suckhoe" />
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Nghề nghiệp:<i class="warning">*</i></span>
                            <input type="text" class="k t ip-avt-t" id="nghenghiep" />
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Trình độ văn hóa:</span>
                            <select class="k t ip-avt-t" id="sl-trinhdovanhoa">
                                <option value="12">Lớp 12</option>
                                <option value="11">Lớp 11</option>
                                <option value="10">Lớp 10</option>
                                <option value="9">Lớp 9</option>
                                <option value="8">Lớp 8</option>
                                <option value="7">Lớp 7</option>
                                <option value="6">Lớp 6</option>
                                <option value="5">Lớp 5</option>
                                <option value="4">Lớp 4</option>
                                <option value="3">Lớp 3</option>
                                <option value="2">Lớp 2</option>
                                <option value="1">Lớp 1</option>
                            </select>
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Chuyên môn:<i class="warning">*</i></span>
                            <input type="text" class="k t ip-avt-t" id="chuyenmon" />
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Bằng cấp: </span>
                            <select class="k t ip-avt-t" id="sl-bangcap">
                                <option value="0"> Không có </option>
                                <option value="1"> Cử nhân/Kĩ sư </option>
                                <option value="2"> Thạc sĩ </option>
                                <option value="3"> Tiến sĩ </option>
                                <option value="4"> >Tiến sĩ </option>
                            </select>
                        </div>
