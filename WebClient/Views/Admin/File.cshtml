                            <select class="k t ip-avt-t" id="sl-liluanchinhtri">
                                <option value="0"> Sơ cấp </option>
                                <option value="1"> Trung cấp </option>
                                <option value="2"> Cao cấp </option>
                            </select>
                        </div>
